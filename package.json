{
  "name": "@saronia/saronia-ui",
  "version": "0.3.0",
  "description": "ui component library",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "private": false,
  "authors": [
    "Daniyar Jakupov",
    "Prasath Soosaithasan <prasath.soosaithasan@gmail.com> (https://ui.saronia.io)"
  ],
  "keywords": [
    "ui",
    "styled-components"
  ],
  "license": "MIT",
  "dependencies": {
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "styled-components": "^1.4.5"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-addon-options": "^1.0.2",
    "autoprefixer": "6.7.2",
    "babel-cli": "6.24.0",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "^20.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.20.0",
    "eslint": "3.16.1",
    "eslint-config-react-app": "^0.6.2",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.4.1",
    "flow-bin": "^0.45.0",
    "jest-cli": "^20.0.0",
    "jest-styled-components": "^2.0.0",
    "json-loader": "0.5.4",
    "np": "^2.14.1",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "watch": "^1.0.2"
  },
  "scripts": {
    "start": "npm run storybook",
    "test": "jest",
    "flow": "flow",
    "dev": "watch 'yarn run build' src",
    "build": "rimraf ./dist && NODE_ENV=production babel src --out-dir build",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "prerelease": "yarn run build",
    "release": "np"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
