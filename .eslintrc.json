{
	"parser": "babel-eslint",
	"extends": "airbnb",
	"env": {
		"node": true,
		"browser": true
	},
  "plugins": ["react", "babel"],
	"rules": {
		"max-len": [2, 100],
		"no-shadow": 0,
		"import/no-extraneous-dependencies": [2, { "devDependencies": true }],
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"react/require-default-props": 0,
	}
}
